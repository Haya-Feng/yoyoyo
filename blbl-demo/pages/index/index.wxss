.nev-s{
  white-space: nowrap;
  padding: 5rpx 0;
}
.nav-item{
  display: inline-block;
  padding: 20rpx 35rpx;
  font-size: 30rpx;
  color: #5e5e5e;
}
.nav-item.active{
  border-bottom: 1rpx solid #000;
}
.slide{
  margin-top: 10rpx;
}
.slide swiper{
  height: 220rpx;
}
.slide navigator{
  width: 100%;
  height: 100%;
}
.slide image{
   width: 100%;
  height: 100%;
}
.video-wrap{
  margin-top: 10rpx;
  display: flex;
  flex-wrap: wrap;
  padding: 5rpx;
  justify-content: space-between;

}
.video-item{
  width: 48%;
  margin-bottom: 10rpx;

}
.v-img{
  position: relative;
}
.v-img image{
  width: 100%;
  border-radius: 15rpx;
}
.v-icon{
  position: absolute;
  bottom: 15rpx;
  left: 0;
  color: #fff;
  font-size: 25rpx;
  display: flex;
  justify-content: space-around;
  width: 100%;

}
.v-desc{
  /* margin-top: 10rpx; */
  font-size: 28rpx;
  display: -webkit-box;
  overflow: hidden;
  white-space: normal;
  text-overflow: ellipsis;
  word-wrap: break-word;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}